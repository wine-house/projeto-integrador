<body>
       <!--HEADER-->
       <header class="header">
        <!--Logo-->
        <div class="logo">
            <figure class="logo__image">
                <img src="/images/menu-footer/logo.png" alt="Logomarca">
            </figure>
        </div>

        <!--Menu-->
        <nav>
            <ul class="navbar">
                <li class="navbar__link"><a href="/">Home</a></li>
                <li class="navbar__link"><a href="/somos">Quem Somos</a></li>
                <li class="navbar__link"><a href="/eventos">Eventos</a></li>
                <li class="navbar__link"><a href="/produtos">Produtos</a></li>
                <li class="navbar__link"><a href="/parceiros">Parceiros</a></li>
            </ul>
        </nav>

        <!--Hamburger Menu-->
        <div class="hamburger">
            <div class="hamburger__line"></div>
            <div class="hamburger__line"></div>
            <div class="hamburger__line"></div>
        </div>

        <!--Login/Compras-->
        <div class="icons">
            <a href="/login">
